{
  "date": {
    "$resolver": "timestamp",
    "pattern": {
      "format": "yyyy-MM-dd HH:mm:ss",
      "timeZone": "UTC"
    }
  },
  "level": {
    "$resolver": "level",
    "field": "name"
  },
  "instanceId": {},
  "applicationContext": {},
  "sourceContext": {
    "$resolver": "logger",
    "field": "name"
  },
  "threadId": {
    "$resolver": "thread",
    "field": "id"
  },
  "message": {
    "$resolver": "message",
    "stringified": true
  },
  "exception": {
    "$resolver": "exception",
    "field": "stackTrace"
  },
  "labels": {
    "$resolver": "mdc",
    "flatten": true,
    "stringified": true
  }
}