{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.13.0"
  },
  "generatorName": "my-kotlin",
  "inputSpec": "./json/api-docs.json",
  "outputDir": "Kotlin",
  "additionalProperties": {
    "packageName": "onlyoffice.docspace.api.sdk",
    "packageVersion": "4.0.0",
    "licenseName": "Apache-2.0",
    "repositoryUrl": "https://github.com/ONLYOFFICE/docspace-api-sdk-kotlin",
    "library": "jvm-retrofit2",
    "dateLibrary": "java8",
    "excludeTests": true
  }
}