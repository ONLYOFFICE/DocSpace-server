{
    "name": "{{{packageName}}}",
    "type": "library",
    "version": "{{{packageVersion}}}",
    "description": "A simple PHP SDK for integrating with the ONLYOFFICE DocSpace API",
    "keywords": [
        "onlyoffice",
        "docspace",
        "sdk"
    ],
    "homepage": "https://www.onlyoffice.com",
    "license": "{{{licenseName}}}",
    "authors": [
        {
            "name": "Ascensio System SIA",
            "homepage": "https://www.onlyoffice.com"
        }
    ],
    "support": {
        "issues": "{{repositoryUrl}}/issues",
        "source": "{{repositoryUrl}}",
        "email": "support@onlyoffice.com",
        "forum": "https://forum.onlyoffice.com/"
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "{{{escapedInvokerPackage}}}\\" : "{{{srcBasePath}}}/" }
    },
    "autoload-dev": {
        "psr-4": { "{{{escapedInvokerPackage}}}\\Test\\" : "{{{testBasePath}}}/" }
    }
}
